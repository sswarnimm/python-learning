b = "Swarnim's"  #use it if you have single quotes in your string
print(b)
print(type(b))

b = 'Swarnim"s' #use when you have double quotes in your string
print(b)
print(type(b))

b = '''Swarnim's" and swarnim"s''' #use when you have single and double quotes in your string.
print(b)
print(type(b))



